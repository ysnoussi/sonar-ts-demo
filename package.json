{
  "name": "sonar-ts-demo",
  "version": "1.0.0",
  "description": "SonarQube Typescript Demo",
  "main": "dist/hello.component.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc",
    "test": "mocha ./src/**/*.spec.ts --recursive --compilers ts:ts-node/register --bail",
    "lint": "tslint --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "dev": "tsc --watch & nodemon dist",
    "sonar": "gulp sonar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuriburger/sonar-ts-demo.git"
  },
  "keywords": [
    "sonar",
    "sonarqube",
    "typescript"
  ],
  "author": "Yuri Burger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuriburger/sonar-ts-demo/issues"
  },
  "homepage": "https://github.com/yuriburger/sonar-ts-demo#readme",
  "dependencies": {
    "@types/chai": "^4.1.6",
    "@types/express": "^4.16.0",
    "@types/node": "^8.0.20",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "nodemon": "1.11.0",
    "ts-node": "^7.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "gulp": "^3.9.1",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.1",
    "karma-typescript": "^3.0.8",
    "sonarqube-scanner": "^2.0.1",
    "tslint": "^5.7.0",
    "tslint-sonarts": "^1.0.0",
    "typescript": "^2.5.2"
  }
}
